<h1>Mis Productos</h1>

<div class="contenedorProductos">
    {{#each productos}}
    <div class="card">
        <p> {{this.title}} </p>
        <p> {{this.description}} </p>
        <p> {{this.price}} </p>
        <button> Comprar </button>
    </div>
    {{/each}}

    <script src="/socket.io/socket.io.js"></script>
</div>